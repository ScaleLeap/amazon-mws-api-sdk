<!--
###########################################################################################################################################################
#  Element ShoeSizeComplianceData Attributes to be filled-in for US/EU/JP marketplaces only when ClothingType is: SHOES, BOOT, SANDAL, TECHNICAL_SPORT_SHOE #. 
   Please refer to the Help Pages for more info
   US:   https://sellercentral.amazon.com/forums/t/update-to-the-feed-templates-for-shoes-listings-in-the-us/456839 #
   UK,ES,FR,IT,DE,IN:  https://sellercentral-europe.amazon.com/gp/help/G200342660
   CA:	https://sellercentral.amazon.ca/gp/help/help.html?itemID=G200342660
   JP : https://sellercentral.amazon.co.jp/forums/t/topic/17987
###########################################################################################################################################################
-->
				<xsd:element name="ShoeSizeComplianceData" minOccurs="0">
                    <xsd:complexType>
                        <xsd:sequence>
						    <xsd:element name="AgeRangeDescription" type="StringNotNull" minOccurs="0"/>
                        <!-- For AgeRangeDescription please select one of the following values US: little_kid, big_kid, toddler, adult, infant ;
                        UK,ES,FR,IT,DE,IN : please select one of the following values adult, kid, baby  
						JP : please select one of the following values 大人, キッズ , ベビー  -->					
							<xsd:element name="FootwearSizeSystem" type="FootwearSizeSystemValues" minOccurs="0"/>
                            <xsd:element name="ShoeSizeAgeGroup" type="ShoeSizeAgeGroupValues" minOccurs="0"/>
							<!-- The attribute 'ShoeSizeGender' is not applicable for JP, EU4 (DE, FR, IT & ES)-->
                            <xsd:element name="ShoeSizeGender" type="ShoeSizeGenderValues" minOccurs="0"/>
                            <xsd:element name="ShoeSizeClass" type="ShoeSizeClassValues" minOccurs="0"/>
							<xsd:element name="ShoeSizeWidth" type="ShoeSizeWidthValues" minOccurs="0"/>
							<xsd:element name="ShoeSize" type="ShoeSizeValues" minOccurs="0"/>
							<xsd:element name="ShoeSizeToRange" type="ShoeSizeValues" minOccurs="0"/>
							<!-- The below attributes are not applicable for JP, EU4 (DE, FR, IT & ES)
							This is intended to capture details for Unisex(Men and Women) separately!
							-->
							<xsd:element name="ShoeSizeGenderUnisex" type="ShoeSizeGenderValues" minOccurs="0"/>
							<!-- ShoeSizeWidthUnisex is not applicable for UK, IN -->							
							<xsd:element name="ShoeSizeWidthUnisex" type="ShoeSizeWidthValues" minOccurs="0"/>
							<xsd:element name="ShoeSizeUnisex" type="ShoeSizeValues" minOccurs="0"/>
							<xsd:element name="ShoeSizeToRangeUnisex" type="ShoeSizeValues" minOccurs="0"/>
						</xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
				

				
				
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>				
